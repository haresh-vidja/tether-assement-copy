# Orchestrator Service - .gitignore
# Service-specific ignore patterns for service orchestration and load balancing

# =============================================================================
# ORCHESTRATION DATA
# =============================================================================

# Service registry data
service_registry/
registry_data/
registered_services/
service_discovery/

# Load balancing data
load_balancer_data/
balancing_data/
routing_data/
distribution_data/

# Health monitoring data
health_data/
health_checks/
monitoring_data/
service_health/

# =============================================================================
# SERVICE DISCOVERY & MANAGEMENT
# =============================================================================

# Service discovery cache
discovery_cache/
service_cache/
peer_cache/
dht_cache/

# Service topology data
topology/
service_topology/
network_topology/
service_graph/

# Service communication logs
communication_logs/
rpc_logs/
service_logs/

# =============================================================================
# LOAD BALANCING ARTIFACTS
# =============================================================================

# Load balancing algorithms data
lb_algorithms/
balancing_algorithms/
routing_algorithms/

# Service capacity data
capacity_data/
resource_data/
performance_data/
utilization_data/

# Request distribution data
distribution_logs/
routing_logs/
allocation_logs/

# =============================================================================
# SERVICE-SPECIFIC DATA
# =============================================================================

# Runtime data directories
data/
temp/
tmp/
storage/

# Service logs
logs/
*.log
*.log.*

# PID files
*.pid

